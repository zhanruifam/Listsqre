<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Normal state -->
    <item android:state_pressed="false">
        <layer-list>
            <item android:drawable="@drawable/edit_button_img" />
        </layer-list>
    </item>

    <!-- Pressed state -->
    <item android:state_pressed="true">
        <layer-list>
            <item android:drawable="@drawable/edit_button_img_pressed" />
        </layer-list>
    </item>
</selector>
